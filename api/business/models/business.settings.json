{
  "kind": "collectionType",
  "collectionName": "businesses",
  "info": {
    "name": "business",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "branches": {
      "type": "component",
      "repeatable": true,
      "component": "custom.branch"
    },
    "hasBranch": {
      "type": "boolean",
      "default": false
    },
    "slogan": {
      "type": "string"
    },
    "address": {
      "type": "string",
      "required": true
    },
    "location": {
      "type": "string",
      "default": "Hargeisa",
      "required": true
    },
    "hasWhatsapp": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "hasMap": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "hasBanner": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "hasLogo": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "hasHours": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "contact": {
      "type": "component",
      "repeatable": false,
      "component": "business.contact",
      "required": true
    },
    "geo": {
      "type": "component",
      "repeatable": false,
      "component": "business.geo"
    },
    "hours": {
      "type": "component",
      "repeatable": false,
      "component": "business.hours"
    },
    "images": {
      "type": "component",
      "repeatable": false,
      "component": "business.images"
    }
  }
}
